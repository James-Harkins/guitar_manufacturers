<div>
  <h1>All Manufacturers</h1>
  <% @manufacturers.each do |manufacturer| %>
    <h2 id="<%= manufacturer.id %>"><%= manufacturer.name %><br>
    <%= link_to 'Update Manufacturer', "/manufacturers/#{manufacturer.id}/edit" %><br>
    <%= link_to 'Delete Manufacturer', "/manufacturers/#{manufacturer.id}", method: :delete %></h2>
    <p>Created at: <%= manufacturer.created_at %></p>
  <% end %>
</div>

<%= link_to 'New Manufacturer', '/manufacturers/new' %><br>

<div>
  <a href="/manufacturers">Manufacturers Index</a><br>
  <a href="/guitars">Guitars Index</a>
</div>
