<%= form_with url: "/manufacturers", method: :post, local: true do |form| %>
  <%= form.label :name %>
  <%= form.text_field :name %><br>
  <%= form.label :headquarter_city %>
  <%= form.text_field :headquarter_city %><br>
  <%= form.label :years_in_business %>
  <%= form.text_field :years_in_business %><br>
  <%= form.label :custom_shop %>
  <%= form.check_box :custom_shop %><br>
  <%= form.submit 'Create Manufacturer' %>
<% end %>
